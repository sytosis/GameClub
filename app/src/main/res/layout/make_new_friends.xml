<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Ui.Authentication.AuthenticationFragment"
    android:background="@drawable/wood_background"
    android:id="@+id/layout">


    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rose"
        android:textColor="#000000"
        android:textSize="34dp"
        app:layout_constraintEnd_toEndOf="@+id/rose_button"
        app:layout_constraintStart_toStartOf="@+id/rose_button"
        app:layout_constraintTop_toBottomOf="@+id/rose_button" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jason"
        android:textColor="#000000"
        android:textSize="34dp"
        app:layout_constraintEnd_toEndOf="@+id/jason_button"
        app:layout_constraintStart_toStartOf="@+id/jason_button"
        app:layout_constraintTop_toBottomOf="@+id/jason_button" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Clay"
        android:textColor="#000000"
        android:textSize="34dp"
        app:layout_constraintEnd_toEndOf="@+id/clay_button"
        app:layout_constraintStart_toStartOf="@+id/clay_button"
        app:layout_constraintTop_toBottomOf="@+id/clay_button" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sue"
        android:textColor="#000000"
        android:textSize="34dp"
        app:layout_constraintEnd_toEndOf="@+id/sue_button"
        app:layout_constraintStart_toStartOf="@+id/sue_button"
        app:layout_constraintTop_toBottomOf="@+id/sue_button" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sam"
        android:textColor="#000000"
        android:textSize="34dp"
        app:layout_constraintEnd_toEndOf="@+id/sam_button"
        app:layout_constraintStart_toStartOf="@+id/sam_button"
        app:layout_constraintTop_toBottomOf="@+id/sam_button" />

    <Button
        android:id="@+id/back_button"
        android:layout_width="126dp"
        android:layout_height="93dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/home_back"
        android:text="Button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="50dp"
        android:text="These are our recommendations, press one!"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="35dp"
        app:layout_constraintBottom_toTopOf="@+id/rose_button"
        app:layout_constraintEnd_toEndOf="@+id/clay_button" />

    <EditText
        android:id="@+id/editText"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_marginTop="50dp"
        android:background="#FFFFFF"
        android:text=" Type Here..."
        android:textColor="#454545"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3">

    </EditText>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="276dp"
        android:text="You can search a \nplayer by typing in\ntheir name or\nE-mail."
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.089"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="#25C4DF"
        android:text="  Search  "
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/editText"
        app:layout_constraintTop_toBottomOf="@+id/editText" />

    <Button
        android:id="@+id/jack_button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginEnd="75dp"
        android:background="@drawable/jack_photo"
        app:layout_constraintBottom_toBottomOf="@+id/rose_button"
        app:layout_constraintEnd_toStartOf="@+id/rose_button"
        app:layout_constraintTop_toTopOf="@+id/rose_button" />

    <Button
        android:id="@+id/rose_button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/rose_photo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.38" />

    <Button
        android:id="@+id/jason_button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="75dp"
        android:background="@drawable/jason_photo"
        app:layout_constraintBottom_toBottomOf="@+id/rose_button"
        app:layout_constraintStart_toEndOf="@+id/rose_button"
        app:layout_constraintTop_toTopOf="@+id/rose_button" />

    <Button
        android:id="@+id/sam_button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginEnd="75dp"
        android:background="@drawable/jay_photo"
        app:layout_constraintBottom_toBottomOf="@+id/sue_button"
        app:layout_constraintEnd_toStartOf="@+id/sue_button"
        app:layout_constraintTop_toTopOf="@+id/sue_button" />

    <Button
        android:id="@+id/sue_button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/sue_photo"
        app:layout_constraintEnd_toEndOf="@+id/rose_button"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/rose_button"
        app:layout_constraintTop_toBottomOf="@+id/rose_button" />

    <Button
        android:id="@+id/clay_button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="75dp"
        android:background="@drawable/clay_photo"
        app:layout_constraintBottom_toBottomOf="@+id/sue_button"
        app:layout_constraintStart_toEndOf="@+id/sue_button"
        app:layout_constraintTop_toTopOf="@+id/sue_button" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jack"
        android:textColor="#000000"
        android:textSize="34dp"
        app:layout_constraintEnd_toEndOf="@+id/jack_button"
        app:layout_constraintStart_toStartOf="@+id/jack_button"
        app:layout_constraintTop_toBottomOf="@+id/jack_button" />

</androidx.constraintlayout.widget.ConstraintLayout>